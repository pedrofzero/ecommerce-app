generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Sneakers {
  createdAt   DateTime @default(now())
  modifiedAt  DateTime @updatedAt
  id          String   @id @default(cuid())
  name        String
  description String
  image       String
  price       Float
}
